{
  "compilerOptions": {
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    // "plugins": [
    //   { "transform": "@zerollup/ts-transform-paths" }
    // ],
    "skipLibCheck": true,
    "strict": true,
    "target": "es5"
  }
}
